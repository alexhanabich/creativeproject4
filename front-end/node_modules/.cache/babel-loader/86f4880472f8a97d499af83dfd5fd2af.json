A,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KAHA;AAIA,IAAA,KAJA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,kBAIA;AACA,MAAA,KAAA,CAAA,kEAAA,CAAA;AACA,KANA;AAOA,IAAA,MAPA,oBAOA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,QAFA,GAEA,IAAA,QAAA,EAFA;AAGA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AAHA;AAAA,uBAIA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CAJA;;AAAA;AAIA,gBAAA,EAJA;AAAA;AAAA,uBAKA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,IAAA,EAAA,EAAA,CAAA,IAAA,CAAA,IAFA;AAGA,kBAAA,WAAA,EAAA,MAAA,CAAA,WAHA;AAIA,kBAAA,KAAA,EAAA,MAAA,CAAA,KAJA;AAKA;AACA,kBAAA,QAAA,EAAA,MAAA,CAAA,QANA,CAOA;AACA;AACA;;AATA,iBAAA,CALA;;AAAA;AAKA,gBAAA,EALA;AAgBA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;AAhBA;AAAA;;AAAA;AAAA;AAAA;AAkBA,gBAAA,OAAA,CAAA,GAAA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBA,KA3BA;AA4BA,IAAA,QA5BA,sBA4BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KApCA;AAqCA,IAAA,UArCA,sBAqCA,IArCA,EAqCA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KAxCA;AAyCA,IAAA,UAzCA,sBAyCA,IAzCA,EAyCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAlDA;AAmDA,IAAA,QAnDA,oBAmDA,IAnDA,EAmDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,IAAA,CAAA,KADA;AAEA,kBAAA,WAAA,EAAA,IAAA,CAAA,WAFA;AAGA,kBAAA,KAAA,EAAA,IAAA,CAAA,KAHA;AAIA,kBAAA,KAAA,EAAA,IAAA,CAAA,KAJA;AAKA,kBAAA,QAAA,EAAA,IAAA,CAAA,QALA,CAMA;AACA;;AAPA,iBAAA,CAFA;;AAAA;AAWA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAZA,kDAaA,IAbA;;AAAA;AAAA;AAAA;AAeA,gBAAA,OAAA,CAAA,GAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA;AApEA;AA3BA,CAAA","sourcesContent":["<template>\n<div class=\"admin\">\n  <h1>Settings!</h1>\n    <div class=\"heading\">\n      <div class=\"circle\">1</div>\n      <h2>What Do You Have in Your Mind?</h2> \n    </div>\n    \n    <div class=\"add\">\n      <div class=\"form\">\n        <div class =\"formTwo\">\n          <input v-model=\"title\" placeholder=\"Title\">\n          <textarea v-model=\"description\" placeholder=\"Description\"></textarea>\n          <p></p>\n        </div>\n        <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n        <div class=\"suggestions\" v-if=\"suggestions.length < 2\">\n          <button @click=\"upload\">Upload</button>\n        </div>\n        <div v-else>\n          <button @click=\"alert\">Upload</button>\n        </div>\n      </div>\n      <div class=\"upload\" v-if=\"addItem\">\n        <h2>{{addItem.title}}</h2>\n        <img :src=\"addItem.path\" />\n        <h3>{{addItem.description}}</h3>\n      </div>\n    </div>\n    <div class=\"heading\">\n  <div class=\"circle\">2</div>\n    <h2>Edit/Delete an Item</h2>\n  </div>\n  <div class=\"edit\">\n    <div class=\"form\">\n      <input v-model=\"findTitle\" placeholder=\"Search\">\n      <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\n        <div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}</div>\n      </div>\n    </div>\n    <div class=\"upload\" v-if=\"findItem\">\n      <input v-model=\"findItem.title\">\n      <p></p>\n      <img :src=\"findItem.path\" />\n      <textarea v-model=\"findItem.description\"></textarea>\n      <div class=\"actions\" v-if=\"findItem\">\n        <button @click=\"deleteItem(findItem)\">Delete</button>\n        <button @click=\"editItem(findItem)\">Edit</button>\n      </div>\n    </div>\n  </div>\n</div>\n  \n</template>\n\n<script>\n  import axios from 'axios';\n  export default {\n    name: 'Admin',\n    data() {\n      return {\n        title: \"\",\n        file: null,\n        addItem: null,\n        items: [],\n        findTitle: \"\",\n        findItem: null,\n        description: \"\",\n        likes: 0,\n        comments: [],\n        //ADD\n        //author: \"\",\n        //text: \"\",\n      }\n    },\n    computed: {\n      suggestions() {\n        let items = this.items.filter(item => item.title.toLowerCase().startsWith(this.findTitle.toLowerCase()));\n        return items.sort((a, b) => a.title > b.title);\n      }\n    },\n    created() {\n      this.getItems();\n    },\n    methods: {\n      fileChanged(event) {\n        this.file = event.target.files[0]\n      },\n      alert() {\n        alert(\"You can add only two menu! Please delete one before you proceed.\")\n      },\n      async upload() {\n        try {\n          const formData = new FormData();\n          formData.append('photo', this.file, this.file.name)\n          let r1 = await axios.post('/api/photos', formData);\n          let r2 = await axios.post('/api/items', {\n            title: this.title,\n            path: r1.data.path,\n            description: this.description,\n            likes: this.likes,\n            //index: this.index,\n            comments: this.comments,\n            //ADD\n            //author: this.author,\n            //text: this.text,\n          });\n          this.addItem = r2.data;\n        } catch (error) {\n          console.log(error);\n        }\n      },\n      async getItems() {\n        try {\n          let response = await axios.get(\"/api/items\");\n          this.items = response.data;\n          return true;\n        } catch (error) {\n          console.log(error);\n        }\n      },\n      selectItem(item) {\n        this.findTitle = \"\";\n        this.findItem = item;\n      },\n      async deleteItem(item) {\n        try {\n          await axios.delete(\"/api/items/\" + item._id);\n          this.findItem = null;\n          this.getItems();\n          return true;\n        } catch (error) {\n          console.log(error);\n        }\n      },\n      async editItem(item) {\n        try {\n          await axios.put(\"/api/items/\" + item._id, {\n            title: item.title,\n            description: item.description,\n            likes: item.likes,\n            index: item.index,\n            comments: item.comments,\n            //author: item.author,\n            //text: item.text,\n          });\n          this.findItem = null;\n          this.getItems();\n          return true;\n        } catch (error) {\n          console.log(error);\n        }\n      },\n    }\n  }\n</script>\n\n<style scoped>\n\n.admin {\n  margin: 0 5em 1.5em 5em;\n}\n.image h2 {\n  font-style: italic;\n  font-size: 1em;\n}\n\n.heading {\n  display: flex;\n  margin-bottom: 20px;\n  margin-top: 20px;\n}\n\n.heading h2 {\n  margin-top: 8px;\n  margin-left: 10px;\n}\n\n.add,\n.edit {\n  display: flex;\n}\n\n.circle {\n  border-radius: 50%;\n  width: 18px;\n  height: 18px;\n  padding: 8px;\n  background: #333;\n  color: #fff;\n  text-align: center\n}\n\n/* Form */\ninput,\ntextarea,\nselect,\nbutton {\n  font-family: 'Indie Flower', cursive;\n  font-size: 1em;\n  margin-bottom: 5pt;\n  margin-top: 5pt;\n}\n\n.form {\n  margin-right: 50px;\n}\n\n/* Uploaded images */\n.upload h2 {\n  margin: 0px;\n}\n\n.upload img {\n  max-width: 300px;\n}\n\n/* Suggestions */\n.suggestions {\n  width: 200px;\n  border: 1px solid #ccc;\n}\n\n.formTwo {\n  display: flex;\n  flex-direction: column;\n}\n\n.suggestion {\n  min-height: 20px;\n}\n\n.suggestion:hover {\n  background-color: #5BDEFF;\n  color: #fff;\n}\n\n.upload {\n  display: flex;\n  flex-direction: column;\n}\n\ntextarea {\n   border: 1px solid #ccc;\n}\n</style>\n\n"],"sourceRoot":"src/views"}]}